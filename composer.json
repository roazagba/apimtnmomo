        "mtn momo",
        "api mtn momo"
    ],
    "type": "library",
    "require": {
        "php": ">=8.0.2",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Roazagba\\ApiMTNMomo\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Romeo AZAGBA",
            "email": "ana@roazagba.me",
            "homepage": "https://roazagba.me",
            "role": "Analyst Programmer | Web Developer"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Roazagba\\ApiMTNMomo\\Providers\\MTNMoMoServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    }
